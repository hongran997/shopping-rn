{
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://c-shopping-three.vercel.app/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}